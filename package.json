{
  "name": "my-website",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "b":"npm run build && npm run start",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "d": "npm run md-deploy && npm run algolia-indexing",
    "premd-deploy":"git add -f blog/ docs/ static/assets",
    "md-deploy":"docusaurus deploy",
    "postmd-deploy": "git rm -r --cached docs/ blog/*  static/assets && git add blog/authors.yml",
    "algolia-indexing":"docker run --platform linux/amd64 -it --env-file=./algolia/.env -e CONFIG=\"$(cat ./algolia/config.json | jq -r tostring)\" algolia/docsearch-scraper",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@cookbookdev/docusaurus-jsx-runtime-fallback-plugin": "^1.0.1",
    "@docusaurus/core": "2.4.1",
    "@docusaurus/preset-classic": "2.4.1",
    "@docusaurus/theme-mermaid": "2.4.1",
    "@giscus/react": "^3.0.0",
    "@mdx-js/react": "^1.6.22",
    "clsx": "^1.2.1",
    "hast-util-is-element": "^1.1.0",
    "meilisearch-docsearch": "^0.4.7",
    "prism-react-renderer": "^1.3.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rehype-katex": "^5.0.0",
    "remark-math": "^3.0.1"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "2.4.1",
    "@tsconfig/docusaurus": "^1.0.5",
    "typescript": "^4.7.4"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=16.14"
  }
}
